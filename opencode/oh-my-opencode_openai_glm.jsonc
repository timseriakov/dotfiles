{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    "sisyphus": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
      "description": "Главный оркестратор. Планирует, делегирует и управляет задачами.",
    },
    "atlas": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
    },
    "oracle": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
    },
    "librarian": {
      "model": "zai-coding-plan/glm-4.7-flash",
      "temperature": 0.2,
    },
    "explore": {
      "model": "zai-coding-plan/glm-4.7-flash",
      "temperature": 0.1,
    },
    "frontend-ui-ux-engineer": {
      "model": "openai/gpt-5.2",
      "temperature": 0.4,
    },
    "document-writer": {
      "model": "zai-coding-plan/glm-4.7-flash",
      "temperature": 0.3,
    },
    "multimodal-looker": {
      "model": "zai-coding-plan/glm-4.6v",
      "temperature": 0.1,
    },
    "build": {
      "model": "openai/gpt-5.1-codex-max",
      "temperature": 0.05,
    },
    "plan": {
      "model": "openai/gpt-5.2",
      "temperature": 0.2,
    },
    "OpenCode-Builder": {
      "model": "openai/gpt-5.2-codex",
      "temperature": 0.05,
    },
    "prometheus": {
      "model": "openai/gpt-5.2",
      "temperature": 0.2,
    },
    "metis": {
      "model": "openai/gpt-5.2",
      "temperature": 0.3,
    },
    "momus": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
    },
  },
  "sisyphus_agent": {
    "disabled": false,
    "default_builder_enabled": false,
    "planner_enabled": true,
    "replace_plan": true,
  },
  "ralph_loop": {
    "enabled": true,
    "default_max_iterations": 100,
  },
  "background_task": {
    "defaultConcurrency": 5,
    "providerConcurrency": {
      "openai": 4,
      "zai-coding-plan": 4,
    },
    "modelConcurrency": {
      "zai-coding-plan/glm-4.7-flash": 8,
      "zai-coding-plan/glm-4.6v": 3,
      "openai/gpt-5.2": 2,
      "openai/gpt-5.2-codex": 1,
      "openai/gpt-5.1-codex-max": 2,
    },
  },
  "categories": {
    "visual": {
      "model": "openai/gpt-5.2",
      "temperature": 0.7,
      "prompt_append": "Focus on modern aesthetics, responsiveness, and clean CSS/Tailwind.",
    },
    "business-logic": {
      "model": "openai/gpt-5.1-codex-max",
      "temperature": 0.1,
      "prompt_append": "Focus on robustness, error handling, and type safety.",
    },
    "architecture": {
      "model": "openai/gpt-5.2",
      "temperature": 0.15,
      "prompt_append": "For architecture tasks, use $likec4-architecture. Prefer LikeC4 DSL changes, validate with `npx likec4 validate`, and return preview/build/export commands.",
    },
    "quick": {
      "model": "zai-coding-plan/glm-4.7-flash",
      "temperature": 0.3,
    },
  },
  "claude_code": {
    "mcp": false,
    "commands": false,
    "skills": true,
    "agents": true,
    "hooks": false,
    "plugins": true,
    "plugins_override": {},
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false,
  },
  "experimental": {
    "auto_resume": true,
    "aggressive_truncation": true,
    "truncate_all_tool_outputs": true,
    "preemptive_compaction": true,
    "dynamic_context_pruning": {
      "enabled": true,
      "notification": "detailed",
      "turn_protection": {
        "enabled": true,
        "turns": 3,
      },
    },
  },
  "disabled_mcps": [],
  "disabled_skills": ["playwright"],
  "disabled_agents": [],
  "disabled_hooks": [],
  "google_auth": false,
  "lsp": {},
}
