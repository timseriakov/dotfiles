{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    "sisyphus": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
      "description": "Главный оркестратор. Планирует, делегирует и управляет задачами.",
    },
    "atlas": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
    },
    "oracle": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "frontend-ui-ux-engineer": {
      "model": "google/antigravity-gemini-3-pro-high",
    },
    "document-writer": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "build": {
      "model": "openai/gpt-5.2",
    },
    "plan": {
      "model": "openai/gpt-5.2",
    },
    "OpenCode-Builder": {
      "model": "openai/gpt-5.2",
    },
    "prometheus": {
      "model": "openai/gpt-5.2",
      "temperature": 0.2,
    },
    "metis": {
      "model": "openai/gpt-5.2",
      "temperature": 0.3,
    },
    "momus": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
    },
  },
  "sisyphus_agent": {
    "disabled": false,
    "default_builder_enabled": false,
    "planner_enabled": true,
    "replace_plan": true,
  },
  "ralph_loop": {
    "enabled": true,
    "default_max_iterations": 100,
  },
  "background_task": {
    "defaultConcurrency": 5,
    "providerConcurrency": {
      "anthropic": 3,
      "openai": 5,
      "google": 10,
    },
    "modelConcurrency": {
      "anthropic/claude-opus-4-6": 2,
      "google/gemini-3-flash": 10,
    },
  },
  "categories": {
    "visual": {
      "model": "google/gemini-3-pro-preview",
      "temperature": 0.7,
      "prompt_append": "Focus on modern aesthetics, responsiveness, and clean CSS/Tailwind.",
    },
    "business-logic": {
      "model": "openai/gpt-5.2",
      "temperature": 0.1,
      "prompt_append": "Focus on robustness, error handling, and type safety.",
    },
    "quick": {
      "model": "google/gemini-3-flash",
      "temperature": 0.3,
    },
  },
  "claude_code": {
    "mcp": true,
    "commands": true,
    "skills": true,
    "agents": true,
    "hooks": true,
    "plugins": true,
    "plugins_override": {},
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false,
  },
  "experimental": {
    "auto_resume": true,
    "aggressive_truncation": true,
    "truncate_all_tool_outputs": true,
    "preemptive_compaction": true,
    "dynamic_context_pruning": {
      "enabled": true,
      "notification": "detailed",
      "turn_protection": {
        "enabled": true,
        "turns": 3,
      },
    },
  },
  "disabled_mcps": [],
  "disabled_skills": [],
  "disabled_agents": [],
  "disabled_hooks": [],
  "google_auth": false,
  "lsp": {},
}
